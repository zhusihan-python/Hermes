<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:adminTools="clr-namespace:Hermes.Features.AdminTools"
             xmlns:suki="https://github.com/kikipoulet/SukiUI"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="750"
             x:DataType="adminTools:AdminToolsViewModel"
             x:Class="Hermes.Features.AdminTools.AdminToolsView">

    <ScrollViewer VerticalScrollBarVisibility="Auto">

        <WrapPanel Margin="{StaticResource DefaultMargin}"
                   suki:WrapPanelExtensions.AnimatedScroll="True"
                   Orientation="Horizontal">
            <!--<adminTools:FeaturePermissionsView DataContext="{Binding FeaturePermissionsViewModel}" />-->
			<adminTools:SystemHelperView/>
        </WrapPanel>
    </ScrollViewer>
</UserControl>